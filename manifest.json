{
  "manifest_version": 2,
  "default_locale": "en",

  "name": "Insta-Tweet",
  "description": "This extension will allow you to Tweet selected text in one click.",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab",
   "contextMenus"
   ]
}